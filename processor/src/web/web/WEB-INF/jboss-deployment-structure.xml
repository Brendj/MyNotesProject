<jboss-deployment-structure xmlns="urn:jboss:deployment-structure:1.0">
    <deployment>
        <dependencies>
            <module name="org.apache.commons.logging"/>
            <module name="org.apache.commons.collections"/>
            <module name="org.apache.xerces"/>
            <module name="org.apache.httpcomponents"/>
            <module name="org.bouncycastle"/>
            <module name="org.hibernate"/>
            <module name="org.slf4j"/>
            <module name="org.apache.cxf"/>
            <module name="javax.servlet.api"/>
            <module name="ecafe"/>
            <module name="ru.cryptopro" />
            <module name="com.google.zxing"/>
            <module name="com.google.guava"/>
            <module name="com.google.visualization.datasource">
                <imports>
                    <include path="**"/>
                </imports>
            </module>
            <module name="com.ibm.icu"/>
            <module name="com.ibm.mq"/>
            <module name="org.apache.ws.security"/>
            <module name="org.springframework.spring">
                <imports>
                    <include path="**"/>
                </imports>
            </module>
            <module name="javax.faces.api" slot="1.2" />
            <module name="com.sun.jsf-impl" slot="1.2" />
            <module name="org.infinispan" />
            <module name="org.codehaus.jackson.jackson-jaxrs" />
            <module name="org.codehaus.jackson.jackson-core-asl" />
            <module name="org.codehaus.jackson.jackson-mapper-asl" />
            <module name="org.codehaus.jackson.jackson-xc" />
            <module name="net.sf.jasperreports" />
            <module name="net.sf.jasperreports-fonts" />
            <module name="net.sourceforge.dynamicreports" />
            <module name="org.apache.commons.digester"/>
            <module name="org.apache.poi"/>
        </dependencies>
        <exclusions>
            <module name="javax.faces.api" slot="main" />
            <module name="com.sun.jsf-impl" slot="main" />
        </exclusions>
    </deployment>
</jboss-deployment-structure>


<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
              version="1.2">

    <application>
        <locale-config>
            <default-locale>ru</default-locale>
        </locale-config>
    </application>

    <application>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
    </application>
    <application>
        <variable-resolver>
            org.springframework.web.jsf.DelegatingVariableResolver
        </variable-resolver>
    </application>


    <managed-bean>
        <display-name>mainPage</display-name>
        <managed-bean-name>mainPage</managed-bean-name>
        <managed-bean-class>ru.axetta.ecafe.processor.web.ui.MainPage</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <converter>
        <description>Org state converter</description>
        <converter-id>orgStateConverter</converter-id>
        <converter-class>ru.axetta.ecafe.processor.web.ui.org.OrgStateConverter</converter-class>
    </converter>

    <converter>
        <description>Timestamp converter with second accuracy</description>
        <converter-id>timeConverter</converter-id>
        <converter-class>ru.axetta.ecafe.processor.web.ui.converter.TimeConverter</converter-class>
    </converter>

    <converter>
        <description>Timestamp converter with minute accuracy</description>
        <converter-id>timeMinuteConverter</converter-id>
        <converter-class>ru.axetta.ecafe.processor.web.ui.converter.TimeMinuteConverter</converter-class>
    </converter>

    <converter>
        <description>Date to time converter</description>
        <converter-id>dateConverter</converter-id>
        <converter-class>ru.axetta.ecafe.processor.web.ui.converter.DateConverter</converter-class>

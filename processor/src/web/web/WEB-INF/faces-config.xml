    </converter>

    <navigation-rule>
        <from-view-id>/back-office/index.jsp</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/back-office/index.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>


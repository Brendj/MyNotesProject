<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfComplexinfo" table="cf_complexinfo" schema="public">
        <id name="idofcomplexinfo">
            <column name="idofcomplexinfo" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property name="idofcomplex">
            <column name="idofcomplex" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="idoforg">
            <column name="idoforg" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="complexname">
            <column name="complexname" sql-type="varchar" length="60" not-null="true"/>
        </property>
        <property name="menudate">
            <column name="menudate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="modefree">
            <column name="modefree" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="modegrant">
            <column name="modegrant" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="modeofadd">
            <column name="modeofadd" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="usetrdiscount">
            <column name="usetrdiscount" sql-type="int4" length="10"/>
        </property>
        <property name="idofdiscountdetail">
            <column name="idofdiscountdetail" sql-type="int8" length="19"/>
        </property>
        <property name="idofmenudetail">
            <column name="idofmenudetail" sql-type="int8" length="19"/>
        </property>
        <property name="currentprice">
            <column name="currentprice" sql-type="int8" length="19"/>
        </property>
        <property name="idofgood">
            <column name="idofgood" sql-type="int8" length="19"/>
        </property>
        <many-to-one name="cfGoodsByIdofgood" class="ru.axetta.ecafe.processor.core.persistence.CfGoods">
            <column name="idofgood"/>
        </many-to-one>
        <many-to-one name="cfMenudetailsByIdofmenudetail"
                     class="ru.axetta.ecafe.processor.core.persistence.CfMenudetails">
            <column name="idofmenudetail"/>
        </many-to-one>
        <many-to-one name="cfOrgsByIdoforg" class="ru.axetta.ecafe.processor.core.persistence.CfOrgs">
            <column name="idoforg" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfDiscountrulesCategorydiscounts"
           table="cf_discountrules_categorydiscounts" schema="public">
        <composite-id mapped="true"
                      class="ru.axetta.ecafe.processor.core.persistence.CfDiscountrulesCategorydiscountsPK">
            <key-property name="idofrule">
                <column name="idofrule" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="idofcategorydiscount">
                <column name="idofcategorydiscount" sql-type="int8" length="19" not-null="true"/>
            </key-property>
        </composite-id>
        <property insert="false" update="false" name="idofdrcd">
            <column name="idofdrcd" sql-type="bigserial" length="19" not-null="true"/>
        </property>
        <many-to-one name="cfCategorydiscountsByIdofcategorydiscount"
                     class="ru.axetta.ecafe.processor.core.persistence.CfCategorydiscounts">
            <column name="idofcategorydiscount" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfDiscountrulesByIdofrule"
                     class="ru.axetta.ecafe.processor.core.persistence.CfDiscountrules">
            <column name="idofrule" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
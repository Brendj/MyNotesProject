<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfTechnologicalMapProducts"
           table="cf_technological_map_products" schema="public">
        <id name="idoftechnomapproducts">
            <column name="idoftechnomapproducts" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property insert="false" update="false" name="idoftechnologicalmaps">
            <column name="idoftechnologicalmaps" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idofproducts">
            <column name="idofproducts" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="netweight">
            <column name="netweight" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="grossweight">
            <column name="grossweight" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property insert="false" update="false" name="idofconfigurationprovider">
            <column name="idofconfigurationprovider" sql-type="int8" length="19"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <property name="numbergroupreplace">
            <column name="numbergroupreplace" sql-type="int4" length="10"/>
        </property>
        <many-to-one name="cfProductsByIdofproducts" class="ru.axetta.ecafe.processor.core.persistence.CfProducts">
            <column name="idofproducts" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfTechnologicalMapByIdoftechnologicalmaps"
                     class="ru.axetta.ecafe.processor.core.persistence.CfTechnologicalMap">
            <column name="idoftechnologicalmaps" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
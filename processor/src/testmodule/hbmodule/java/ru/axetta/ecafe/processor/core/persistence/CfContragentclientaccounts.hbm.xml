<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfContragentclientaccounts"
           table="cf_contragentclientaccounts" schema="public">
        <composite-id mapped="true" class="ru.axetta.ecafe.processor.core.persistence.CfContragentclientaccountsPK">
            <key-property name="idofcontragent">
                <column name="idofcontragent" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="idofaccount">
                <column name="idofaccount" sql-type="int8" length="19" not-null="true"/>
            </key-property>
        </composite-id>
        <property insert="false" update="false" name="idofclient">
            <column name="idofclient" sql-type="int8" length="19" not-null="true"/>
        </property>
        <many-to-one name="cfClientsByIdofclient" class="ru.axetta.ecafe.processor.core.persistence.CfClients">
            <column name="idofclient" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfContragentsByIdofcontragent"
                     class="ru.axetta.ecafe.processor.core.persistence.CfContragents">
            <column name="idofcontragent" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
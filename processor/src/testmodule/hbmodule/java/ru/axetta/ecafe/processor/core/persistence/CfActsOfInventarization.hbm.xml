        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <property name="numberofact">
            <column name="numberofact" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property name="commission">
            <column name="commission" sql-type="varchar" length="512" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
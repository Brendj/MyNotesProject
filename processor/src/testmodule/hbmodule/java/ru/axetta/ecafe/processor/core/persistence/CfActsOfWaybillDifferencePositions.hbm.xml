<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfActsOfWaybillDifferencePositions"
           table="cf_acts_of_waybill_difference_positions" schema="public">
        <id name="idofactofdifferenceposition">
            <column name="idofactofdifferenceposition" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property insert="false" update="false" name="idofactofdifference">
            <column name="idofactofdifference" sql-type="int8" length="19"/>
        </property>
        <property insert="false" update="false" name="idofgood">
            <column name="idofgood" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="unitsscale">
            <column name="unitsscale" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="totalcount">
            <column name="totalcount" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="netweight">
            <column name="netweight" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="grossweight">
            <column name="grossweight" sql-type="int8" length="19"/>
        </property>
        <property name="goodscreationdate">
            <column name="goodscreationdate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="lifetime">
            <column name="lifetime" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="price">
            <column name="price" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="nds">
            <column name="nds" sql-type="int8" length="19"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <many-to-one name="cfActsOfWaybillDifferenceByIdofactofdifference"
                     class="ru.axetta.ecafe.processor.core.persistence.CfActsOfWaybillDifference">
            <column name="idofactofdifference"/>
        </many-to-one>
        <many-to-one name="cfGoodsByIdofgood" class="ru.axetta.ecafe.processor.core.persistence.CfGoods">
            <column name="idofgood" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
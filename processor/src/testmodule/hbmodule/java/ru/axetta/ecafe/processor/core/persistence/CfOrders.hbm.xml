<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfOrders" table="cf_orders" schema="public">
        <composite-id mapped="true" class="ru.axetta.ecafe.processor.core.persistence.CfOrdersPK">
            <key-property name="idoforg">
                <column name="idoforg" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="idoforder">
                <column name="idoforder" sql-type="int8" length="19" not-null="true"/>
            </key-property>
        </composite-id>
        <property name="idofcard">
            <column name="idofcard" sql-type="int8" length="19"/>
        </property>
        <property name="idofclient">
            <column name="idofclient" sql-type="int8" length="19"/>
        </property>
        <property name="idoftransaction">
            <column name="idoftransaction" sql-type="int8" length="19"/>
        </property>
        <property name="idofcashier">
            <column name="idofcashier" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="socdiscount">
            <column name="socdiscount" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="grantsum">
            <column name="grantsum" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="rsum">
            <column name="rsum" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="sumbycard">
            <column name="sumbycard" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="sumbycash">
            <column name="sumbycash" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="idofpos">
            <column name="idofpos" sql-type="int8" length="19"/>
        </property>
        <property name="idofcontragent">
            <column name="idofcontragent" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="trddiscount">
            <column name="trddiscount" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int4" length="10" not-null="true"/>
        </property>
        <many-to-one name="cfCardsByIdofcard" class="ru.axetta.ecafe.processor.core.persistence.CfCards">
            <column name="idofcard"/>
        </many-to-one>
        <many-to-one name="cfClientsByIdofclient" class="ru.axetta.ecafe.processor.core.persistence.CfClients">
            <column name="idofclient"/>
        </many-to-one>
        <many-to-one name="cfContragentsByIdofcontragent"
                     class="ru.axetta.ecafe.processor.core.persistence.CfContragents">
            <column name="idofcontragent" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfOrgsByIdoforg" class="ru.axetta.ecafe.processor.core.persistence.CfOrgs">
            <column name="idoforg" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfPosByIdofpos" class="ru.axetta.ecafe.processor.core.persistence.CfPos">
            <column name="idofpos"/>
        </many-to-one>
        <many-to-one name="cfTransactionsByIdoftransaction"
                     class="ru.axetta.ecafe.processor.core.persistence.CfTransactions">
            <column name="idoftransaction"/>
        </many-to-one>
        <set name="cfOrderdetailses" inverse="true">
            <key>
                <column name="idoforg" not-null="true"/>
                <column name="idoforder" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfOrderdetails"/>
        </set>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfPermissions" table="cf_permissions" schema="public">
        <composite-id mapped="true" class="ru.axetta.ecafe.processor.core.persistence.CfPermissionsPK">
            <key-property name="idofuser">
                <column name="idofuser" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="idoffunction">
                <column name="idoffunction" sql-type="int8" length="19" not-null="true"/>
            </key-property>
        </composite-id>
        <many-to-one name="cfFunctionsByIdoffunction" class="ru.axetta.ecafe.processor.core.persistence.CfFunctions">
            <column name="idoffunction" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfUsersByIdofuser" class="ru.axetta.ecafe.processor.core.persistence.CfUsers">
            <column name="idofuser" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
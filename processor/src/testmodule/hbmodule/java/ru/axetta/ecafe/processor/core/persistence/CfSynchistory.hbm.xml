<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfSynchistory" table="cf_synchistory" schema="public">
        <id name="idofsync">
            <column name="idofsync" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property name="idoforg">
            <column name="idoforg" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="syncstarttime">
            <column name="syncstarttime" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="syncendtime">
            <column name="syncendtime" sql-type="int8" length="19"/>
        </property>
        <property name="syncresult">
            <column name="syncresult" sql-type="int4" length="10"/>
        </property>
        <property name="idofpacket">
            <column name="idofpacket" sql-type="int8" length="19" not-null="true"/>
        </property>
        <many-to-one name="cfOrgsByIdoforg" class="ru.axetta.ecafe.processor.core.persistence.CfOrgs">
            <column name="idoforg" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfDiaryvalues" table="cf_diaryvalues" schema="public">
        <composite-id mapped="true" class="ru.axetta.ecafe.processor.core.persistence.CfDiaryvaluesPK">
            <key-property name="idoforg">
                <column name="idoforg" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="idofclient">
                <column name="idofclient" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="idofclass">
                <column name="idofclass" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="recdate">
                <column name="recdate" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="vtype">
                <column name="vtype" sql-type="int4" length="10" not-null="true"/>
            </key-property>
        </composite-id>
        <property name="value">
            <column name="value" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <many-to-one name="cfClientsByIdofclient" class="ru.axetta.ecafe.processor.core.persistence.CfClients">
            <column name="idofclient" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfDiaryclasses" class="ru.axetta.ecafe.processor.core.persistence.CfDiaryclasses">
            <column name="idoforg" not-null="true"/>
            <column name="idofclass" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfOrgsByIdoforg" class="ru.axetta.ecafe.processor.core.persistence.CfOrgs">
            <column name="idoforg" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
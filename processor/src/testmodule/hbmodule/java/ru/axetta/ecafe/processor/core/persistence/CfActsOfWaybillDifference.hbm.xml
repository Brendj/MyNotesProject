<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfActsOfWaybillDifference"
           table="cf_acts_of_waybill_difference" schema="public">
        <id name="idofactofdifference">
            <column name="idofactofdifference" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="idofstaff">
            <column name="idofstaff" sql-type="int8" length="19"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="dateofactofdifference">
            <column name="dateofactofdifference" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="numberofactofdifference">
            <column name="numberofactofdifference" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <many-to-one name="cfStaffsByIdofstaff" class="ru.axetta.ecafe.processor.core.persistence.CfStaffs">
            <column name="idofstaff"/>
        </many-to-one>
        <set name="cfActsOfWaybillDifferencePositionsesByIdofactofdifference" inverse="true">
            <key>
                <column name="idofactofdifference"/>
            </key>
            <one-to-many not-found="ignore"
                         class="ru.axetta.ecafe.processor.core.persistence.CfActsOfWaybillDifferencePositions"/>
        </set>
    </class>
</hibernate-mapping>
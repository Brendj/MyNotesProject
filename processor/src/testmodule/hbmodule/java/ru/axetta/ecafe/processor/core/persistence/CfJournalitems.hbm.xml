<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfJournalitems" table="cf_journalitems" schema="public">
        <id name="idofjournalitem">
            <column name="idofjournalitem" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="idofjournal">
            <column name="idofjournal" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="idoffund">
            <column name="idoffund" sql-type="int8" length="19"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date" length="13" not-null="true"/>
        </property>
        <property name="number">
            <column name="number" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="cost">
            <column name="cost" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="idofksu1Record">
            <column name="idofksu1record" sql-type="int8" length="19"/>
        </property>
        <property name="idofksu2Record">
            <column name="idofksu2record" sql-type="int8" length="19"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <many-to-one name="cfFundsByIdoffund" class="ru.axetta.ecafe.processor.core.persistence.CfFunds">
            <column name="idoffund"/>
        </many-to-one>
        <many-to-one name="cfJournalsByIdofjournal" class="ru.axetta.ecafe.processor.core.persistence.CfJournals">
            <column name="idofjournal" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfKsu1RecordsByIdofksu1Record"
                     class="ru.axetta.ecafe.processor.core.persistence.CfKsu1Records">
            <column name="idofksu1record"/>
        </many-to-one>
        <many-to-one name="cfKsu2RecordsByIdofksu2Record"
                     class="ru.axetta.ecafe.processor.core.persistence.CfKsu2Records">
            <column name="idofksu2record"/>
        </many-to-one>
    </class>
</hibernate-mapping>
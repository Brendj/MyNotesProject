<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfFriendlyOrganization" table="cf_friendly_organization"
           schema="public">
        <id name="idoffriendlyorg">
            <column name="idoffriendlyorg" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="currentorg" insert="false" update="false">
            <column name="currentorg" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="friendlyorg" insert="false" update="false">
            <column name="friendlyorg" sql-type="int8" length="19" not-null="true"/>
        </property>
        <many-to-one name="cfOrgsByCurrentorg" class="ru.axetta.ecafe.processor.core.persistence.CfOrgs">
            <column name="currentorg" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfOrgsByFriendlyorg" class="ru.axetta.ecafe.processor.core.persistence.CfOrgs">
            <column name="friendlyorg" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
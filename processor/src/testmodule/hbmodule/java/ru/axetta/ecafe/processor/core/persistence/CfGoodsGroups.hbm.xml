<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfGoodsGroups" table="cf_goods_groups" schema="public">
        <id name="idofgoodsgroup">
            <column name="idofgoodsgroup" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="nameofgoodsgroup">
            <column name="nameofgoodsgroup" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <set name="cfDishProhibitionExclusionsesByIdofgoodsgroup" inverse="true">
            <key>
                <column name="idofgoodsgroup"/>
            </key>
            <one-to-many not-found="ignore"
                         class="ru.axetta.ecafe.processor.core.persistence.CfDishProhibitionExclusions"/>
        </set>
        <set name="cfDishProhibitionsesByIdofgoodsgroup" inverse="true">
            <key>
                <column name="idofgoodsgroup"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfDishProhibitions"/>
        </set>
        <set name="cfGoodsesByIdofgoodsgroup" inverse="true">
            <key>
                <column name="idofgoodsgroup" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfGoods"/>
        </set>
    </class>
</hibernate-mapping>
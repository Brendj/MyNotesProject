<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfMenudetails" table="cf_menudetails" schema="public">
        <id name="idofmenudetail">
            <column name="idofmenudetail" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property insert="false" update="false" name="idofmenu">
            <column name="idofmenu" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="menupath">
            <column name="menupath" sql-type="varchar" length="512" not-null="true"/>
        </property>
        <property name="menudetailname">
            <column name="menudetailname" sql-type="varchar" length="512" not-null="true"/>
        </property>
        <property name="groupname">
            <column name="groupname" sql-type="varchar" length="60" not-null="true"/>
        </property>
        <property name="menudetailoutput">
            <column name="menudetailoutput" sql-type="varchar" length="32"/>
        </property>
        <property name="price">
            <column name="price" sql-type="int8" length="19"/>
        </property>
        <property name="menuorigin">
            <column name="menuorigin" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="availablenow">
            <column name="availablenow" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="localidofmenu">
            <column name="localidofmenu" sql-type="int8" length="19"/>
        </property>
        <property name="protein">
            <column name="protein" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="fat">
            <column name="fat" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="carbohydrates">
            <column name="carbohydrates" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="calories">
            <column name="calories" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="vitb1">
            <column name="vitb1" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="vitc">
            <column name="vitc" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="vita">
            <column name="vita" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="vite">
            <column name="vite" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="minca">
            <column name="minca" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="minp">
            <column name="minp" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="minmg">
            <column name="minmg" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="minfe">
            <column name="minfe" sql-type="numeric" length="5" precision="2"/>
        </property>
        <property name="flags">
            <column name="flags" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="priority">
            <column name="priority" sql-type="int4" length="10"/>
        </property>
        <property insert="false" update="false" name="idofgood">
            <column name="idofgood" sql-type="int8" length="19"/>
        </property>
        <many-to-one name="cfGoodsByIdofgood" class="ru.axetta.ecafe.processor.core.persistence.CfGoods">
            <column name="idofgood"/>
        </many-to-one>
        <many-to-one name="cfMenuByIdofmenu" class="ru.axetta.ecafe.processor.core.persistence.CfMenu">
            <column name="idofmenu" not-null="true"/>
        </many-to-one>
        <set name="cfComplexinfosByIdofmenudetail" inverse="true">
            <key>
                <column name="idofmenudetail"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfComplexinfo"/>
        </set>
        <set name="cfComplexinfodetailsByIdofmenudetail" inverse="true">
            <key>
                <column name="idofmenudetail" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfComplexinfodetail"/>
        </set>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfBanks" table="cf_banks" schema="public">
        <id name="idofbank">
            <column name="idofbank" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="128"/>
        </property>
        <property name="logourl">
            <column name="logourl" sql-type="varchar" length="128"/>
        </property>
        <property name="terminalsurl">
            <column name="terminalsurl" sql-type="varchar" length="128"/>
        </property>
        <property name="enrollmenttype">
            <column name="enrollmenttype" sql-type="varchar" length="128"/>
        </property>
        <property name="minrate">
            <column name="minrate" sql-type="float8" length="17" precision="17"/>
        </property>
        <property name="rate">
            <column name="rate" sql-type="float8" length="17" precision="17"/>
        </property>
    </class>
</hibernate-mapping>
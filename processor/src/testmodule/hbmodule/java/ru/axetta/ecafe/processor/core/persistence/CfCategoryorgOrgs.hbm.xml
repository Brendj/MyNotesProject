<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfCategoryorgOrgs" table="cf_categoryorg_orgs"
           schema="public">
        <id name="idoforgscategories">
            <column name="idoforgscategories" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="idoforg">
            <column name="idoforg" sql-type="int8" length="19"/>
        </property>
        <property name="idofcategoryorg">
            <column name="idofcategoryorg" sql-type="int8" length="19"/>
        </property>
        <many-to-one name="cfCategoryorgByIdofcategoryorg"
                     class="ru.axetta.ecafe.processor.core.persistence.CfCategoryorg">
            <column name="idofcategoryorg"/>
        </many-to-one>
        <many-to-one name="cfOrgsByIdoforg" class="ru.axetta.ecafe.processor.core.persistence.CfOrgs">
            <column name="idoforg"/>
        </many-to-one>
    </class>
</hibernate-mapping>
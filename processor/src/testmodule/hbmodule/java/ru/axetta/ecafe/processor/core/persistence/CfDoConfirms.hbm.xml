<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfDoConfirms" table="cf_do_confirms" schema="public">
        <id name="idofdoconfirm">
            <column name="idofdoconfirm" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="distributedobjectclassname">
            <column name="distributedobjectclassname" sql-type="varchar" length="64" not-null="true"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
    </class>
</hibernate-mapping>
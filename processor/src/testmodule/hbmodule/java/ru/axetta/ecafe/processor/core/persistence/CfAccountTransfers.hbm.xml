<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfAccountTransfers" table="cf_account_transfers"
           schema="public">
        <id name="idofaccounttransfer">
            <column name="idofaccounttransfer" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idofclientbenefactor">
            <column name="idofclientbenefactor" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idofclientbeneficiary">
            <column name="idofclientbeneficiary" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="reason">
            <column name="reason" sql-type="varchar" length="256"/>
        </property>
        <property name="createdby">
            <column name="createdby" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idoftransactiononbenefactor">
            <column name="idoftransactiononbenefactor" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idoftransactiononbeneficiary">
            <column name="idoftransactiononbeneficiary" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="transfersum">
            <column name="transfersum" sql-type="int8" length="19" not-null="true"/>
        </property>
        <many-to-one name="cfClientsByIdofclientbeneficiary"
                     class="ru.axetta.ecafe.processor.core.persistence.CfClients">
            <column name="idofclientbeneficiary" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfClientsByIdofclientbenefactor"
                     class="ru.axetta.ecafe.processor.core.persistence.CfClients">
            <column name="idofclientbenefactor" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfTransactionsByIdoftransactiononbeneficiary"
                     class="ru.axetta.ecafe.processor.core.persistence.CfTransactions">
            <column name="idoftransactiononbeneficiary" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfTransactionsByIdoftransactiononbenefactor"
                     class="ru.axetta.ecafe.processor.core.persistence.CfTransactions">
            <column name="idoftransactiononbenefactor" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfCirculations" table="cf_circulations" schema="public">
        <id name="idofcirculation">
            <column name="idofcirculation" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="idofclient">
            <column name="idofclient" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="idofparentcirculation">
            <column name="idofparentcirculation" sql-type="int8" length="19"/>
        </property>
        <property name="idofissuable">
            <column name="idofissuable" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="issuancedate">
            <column name="issuancedate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="refunddate">
            <column name="refunddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="realrefunddate">
            <column name="realrefunddate" sql-type="int8" length="19"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="quantity">
            <column name="quantity" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <many-to-one name="cfClientsByIdofclient" class="ru.axetta.ecafe.processor.core.persistence.CfClients">
            <column name="idofclient" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
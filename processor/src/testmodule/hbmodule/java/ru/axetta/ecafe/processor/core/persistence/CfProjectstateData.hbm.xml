<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfProjectstateData" table="cf_projectstate_data"
           schema="public">
        <composite-id mapped="true" class="ru.axetta.ecafe.processor.core.persistence.CfProjectstateDataPK">
            <key-property name="period">
                <column name="period" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="type">
                <column name="type" sql-type="int4" length="10" not-null="true"/>
            </key-property>
            <key-property name="stringkey">
                <column name="stringkey" sql-type="varchar" length="128" not-null="true"/>
            </key-property>
        </composite-id>
        <property name="stringvalue">
            <column name="stringvalue" sql-type="varchar" length="128"/>
        </property>
        <property name="generationdate">
            <column name="generationdate" sql-type="int8" length="19" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
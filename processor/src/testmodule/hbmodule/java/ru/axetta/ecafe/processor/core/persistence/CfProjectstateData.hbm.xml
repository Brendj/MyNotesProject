            <key-property name="period">
                <column name="period" sql-type="int8" length="19" not-null="true"/>
            </key-property>
            <key-property name="type">
                <column name="type" sql-type="int4" length="10" not-null="true"/>
            </key-property>
            <key-property name="stringkey">
                <column name="stringkey" sql-type="varchar" length="128" not-null="true"/>
            </key-property>
        </composite-id>
        <property name="stringvalue">
            <column name="stringvalue" sql-type="varchar" length="128"/>
        </property>
        <property name="generationdate">
            <column name="generationdate" sql-type="int8" length="19" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
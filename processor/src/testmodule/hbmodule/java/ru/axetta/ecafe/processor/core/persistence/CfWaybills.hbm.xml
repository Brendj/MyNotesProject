<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfWaybills" table="cf_waybills" schema="public">
        <id name="idofwaybill">
            <column name="idofwaybill" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property insert="false" update="false" name="idofstaff">
            <column name="idofstaff" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idofactofdifference">
            <column name="idofactofdifference" sql-type="int8" length="19"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="numberofwaybill">
            <column name="numberofwaybill" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property name="dateofwaybill">
            <column name="dateofwaybill" sql-type="int8" length="19"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="shipper">
            <column name="shipper" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property name="receiver">
            <column name="receiver" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <many-to-one name="cfStaffsByIdofstaff" class="ru.axetta.ecafe.processor.core.persistence.CfStaffs">
            <column name="idofstaff" not-null="true"/>
        </many-to-one>
        <set name="cfWaybillsPositionsesByIdofwaybill" inverse="true">
            <key>
                <column name="idofwaybill" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfWaybillsPositions"/>
        </set>
    </class>
</hibernate-mapping>
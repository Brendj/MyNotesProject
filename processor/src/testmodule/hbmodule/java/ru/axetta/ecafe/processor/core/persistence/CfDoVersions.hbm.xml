<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfDoVersions" table="cf_do_versions" schema="public">
        <id name="idofdoobject">
            <column name="idofdoobject" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="distributedobjectclassname">
            <column name="distributedobjectclassname" sql-type="varchar" length="64"/>
        </property>
        <property name="currentversion">
            <column name="currentversion" sql-type="int8" length="19"/>
        </property>
    </class>
</hibernate-mapping>
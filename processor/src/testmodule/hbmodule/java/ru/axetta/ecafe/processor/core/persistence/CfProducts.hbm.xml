<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfProducts" table="cf_products" schema="public">
        <id name="idofproducts">
            <column name="idofproducts" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="idofproductgroups">
            <column name="idofproductgroups" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="code">
            <column name="code" sql-type="varchar" length="16" not-null="true"/>
        </property>
        <property name="fullname">
            <column name="fullname" sql-type="varchar" length="1024"/>
        </property>
        <property name="productname">
            <column name="productname" sql-type="varchar" length="512"/>
        </property>
        <property name="okpcode">
            <column name="okpcode" sql-type="varchar" length="128"/>
        </property>
        <property name="classificationcode">
            <column name="classificationcode" sql-type="varchar" length="32"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="idofusercreate">
            <column name="idofusercreate" sql-type="int8" length="19"/>
        </property>
        <property name="idofuseredit">
            <column name="idofuseredit" sql-type="int8" length="19"/>
        </property>
        <property name="idofuserdelete">
            <column name="idofuserdelete" sql-type="int8" length="19"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="idofconfigurationprovider">
            <column name="idofconfigurationprovider" sql-type="int8" length="19"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <property name="density">
            <column name="density" sql-type="float8" length="17" precision="17"/>
        </property>
        <many-to-one name="cfProductGroupsByIdofproductgroups"
                     class="ru.axetta.ecafe.processor.core.persistence.CfProductGroups">
            <column name="idofproductgroups" not-null="true"/>
        </many-to-one>
        <set name="cfDishProhibitionsesByIdofproducts" inverse="true">
            <key>
                <column name="idofproducts"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfDishProhibitions"/>
        </set>
        <set name="cfTechnologicalMapProductsesByIdofproducts" inverse="true">
            <key>
                <column name="idofproducts" not-null="true"/>
            </key>
            <one-to-many not-found="ignore"
                         class="ru.axetta.ecafe.processor.core.persistence.CfTechnologicalMapProducts"/>
        </set>
    </class>
</hibernate-mapping>
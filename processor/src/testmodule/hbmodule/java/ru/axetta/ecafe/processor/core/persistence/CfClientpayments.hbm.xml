<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfClientpayments" table="cf_clientpayments"
           schema="public">
        <id name="idofclientpayment">
            <column name="idofclientpayment" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property insert="false" update="false" name="idoftransaction">
            <column name="idoftransaction" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="paymentmethod">
            <column name="paymentmethod" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="paysum">
            <column name="paysum" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="paytype">
            <column name="paytype" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idofcontragent">
            <column name="idofcontragent" sql-type="int8" length="19"/>
        </property>
        <property insert="false" update="false" name="idofpayment">
            <column name="idofpayment" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idofclientpaymentorder">
            <column name="idofclientpaymentorder" sql-type="int8" length="19"/>
        </property>
        <property name="addpaymentmethod">
            <column name="addpaymentmethod" sql-type="varchar" length="1024"/>
        </property>
        <property name="addidofpayment">
            <column name="addidofpayment" sql-type="varchar" length="1024"/>
        </property>
        <property insert="false" update="false" name="idofcontragentreceiver">
            <column name="idofcontragentreceiver" sql-type="int8" length="19"/>
        </property>
        <many-to-one name="cfClientpaymentordersByIdofclientpaymentorder"
                     class="ru.axetta.ecafe.processor.core.persistence.CfClientpaymentorders">
            <column name="idofclientpaymentorder"/>
        </many-to-one>
        <many-to-one name="cfContragentsByIdofcontragent"
                     class="ru.axetta.ecafe.processor.core.persistence.CfContragents">
            <column name="idofcontragent"/>
        </many-to-one>
        <many-to-one name="cfContragentsByIdofcontragentreceiver"
                     class="ru.axetta.ecafe.processor.core.persistence.CfContragents">
            <column name="idofcontragentreceiver"/>
        </many-to-one>
        <many-to-one name="cfTransactionsByIdoftransaction"
                     class="ru.axetta.ecafe.processor.core.persistence.CfTransactions">
            <column name="idoftransaction" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
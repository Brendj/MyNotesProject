<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfPos" table="cf_pos" schema="public">
        <id name="idofpos">
            <column name="idofpos" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property insert="false" update="false" name="idofcontragent">
            <column name="idofcontragent" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar" length="128"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int4" length="10"/>
        </property>
        <property name="flags">
            <column name="flags" sql-type="int4" length="10"/>
        </property>
        <property name="publickey">
            <column name="publickey" sql-type="varchar" length="1024" not-null="true"/>
        </property>
        <many-to-one name="cfContragentsByIdofcontragent"
                     class="ru.axetta.ecafe.processor.core.persistence.CfContragents">
            <column name="idofcontragent" not-null="true"/>
        </many-to-one>
        <set name="cfOrdersesByIdofpos" inverse="true">
            <key>
                <column name="idofpos"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfOrders"/>
        </set>
    </class>
</hibernate-mapping>
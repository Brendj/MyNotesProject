<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfPublications" table="cf_publications" schema="public">
        <id name="idofpublication">
            <column name="idofpublication" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="dataofpublication">
            <column name="dataofpublication" sql-type="bytea" length="2147483647" not-null="true"/>
        </property>
        <property name="author">
            <column name="author" sql-type="varchar"/>
        </property>
        <property name="title">
            <column name="title" sql-type="varchar" length="512"/>
        </property>
        <property name="title2">
            <column name="title2" sql-type="varchar"/>
        </property>
        <property name="publicationdate">
            <column name="publicationdate" sql-type="varchar" length="15"/>
        </property>
        <property name="publisher">
            <column name="publisher" sql-type="varchar"/>
        </property>
        <property name="isbn">
            <column name="isbn" sql-type="varchar"/>
        </property>
        <property name="validisbn">
            <column name="validisbn" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="hash">
            <column name="hash" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <set name="cfJournalsesByIdofpublication" inverse="true">
            <key>
                <column name="idofpublication" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfJournals"/>
        </set>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfUsers" table="cf_users" schema="public">
        <id name="idofuser">
            <column name="idofuser" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property name="version">
            <column name="version" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="username">
            <column name="username" sql-type="varchar" length="64" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property name="lastchange">
            <column name="lastchange" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar" length="32" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idofcontragent">
            <column name="idofcontragent" sql-type="int8" length="19"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar" length="128"/>
        </property>
        <many-to-one name="cfContragentsByIdofcontragent"
                     class="ru.axetta.ecafe.processor.core.persistence.CfContragents">
            <column name="idofcontragent"/>
        </many-to-one>
        <set name="cfPermissionsesByIdofuser" inverse="true">
            <key>
                <column name="idofuser" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfPermissions"/>
        </set>
    </class>
</hibernate-mapping>
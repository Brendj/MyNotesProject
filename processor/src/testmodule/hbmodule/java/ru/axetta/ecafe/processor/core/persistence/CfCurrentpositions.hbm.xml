<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfCurrentpositions" table="cf_currentpositions"
           schema="public">
        <id name="idofposition">
            <column name="idofposition" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property insert="false" update="false" name="idofcontragentdebtor">
            <column name="idofcontragentdebtor" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idofcontragentcreditor">
            <column name="idofcontragentcreditor" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="summa">
            <column name="summa" sql-type="int8" length="19" not-null="true"/>
        </property>
        <many-to-one name="cfContragentsByIdofcontragentcreditor"
                     class="ru.axetta.ecafe.processor.core.persistence.CfContragents">
            <column name="idofcontragentcreditor" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfContragentsByIdofcontragentdebtor"
                     class="ru.axetta.ecafe.processor.core.persistence.CfContragents">
            <column name="idofcontragentdebtor" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfCategoryorg" table="cf_categoryorg" schema="public">
        <id name="idofcategoryorg">
            <column name="idofcategoryorg" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="categoryname">
            <column name="categoryname" sql-type="varchar"/>
        </property>
        <set name="cfCategoryorgOrgsesByIdofcategoryorg" inverse="true">
            <key>
                <column name="idofcategoryorg"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfCategoryorgOrgs"/>
        </set>
        <set name="cfDiscountrulesCategoryorgsByIdofcategoryorg" inverse="true">
            <key>
                <column name="idofcategoryorg"/>
            </key>
            <one-to-many not-found="ignore"
                         class="ru.axetta.ecafe.processor.core.persistence.CfDiscountrulesCategoryorg"/>
        </set>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfNotifications" table="cf_notifications" schema="public">
        <id name="idofnotification">
            <column name="idofnotification" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property insert="false" update="false" name="idofclient">
            <column name="idofclient" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="notificationtime">
            <column name="notificationtime" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="notificationtype">
            <column name="notificationtype" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="notificationtext">
            <column name="notificationtext" sql-type="varchar" length="1024" not-null="true"/>
        </property>
        <many-to-one name="cfClientsByIdofclient" class="ru.axetta.ecafe.processor.core.persistence.CfClients">
            <column name="idofclient" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
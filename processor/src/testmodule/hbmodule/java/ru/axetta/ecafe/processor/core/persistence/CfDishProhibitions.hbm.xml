<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfDishProhibitions" table="cf_dish_prohibitions"
           schema="public">
        <id name="idofprohibition">
            <column name="idofprohibition" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <property insert="false" update="false" name="idofclient">
            <column name="idofclient" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property insert="false" update="false" name="idofproducts">
            <column name="idofproducts" sql-type="int8" length="19"/>
        </property>
        <property insert="false" update="false" name="idofproductgroups">
            <column name="idofproductgroups" sql-type="int8" length="19"/>
        </property>
        <property insert="false" update="false" name="idofgood">
            <column name="idofgood" sql-type="int8" length="19"/>
        </property>
        <property insert="false" update="false" name="idofgoodsgroup">
            <column name="idofgoodsgroup" sql-type="int8" length="19"/>
        </property>
        <many-to-one name="cfClientsByIdofclient" class="ru.axetta.ecafe.processor.core.persistence.CfClients">
            <column name="idofclient" not-null="true"/>
        </many-to-one>
        <many-to-one name="cfGoodsByIdofgood" class="ru.axetta.ecafe.processor.core.persistence.CfGoods">
            <column name="idofgood"/>
        </many-to-one>
        <many-to-one name="cfGoodsGroupsByIdofgoodsgroup"
                     class="ru.axetta.ecafe.processor.core.persistence.CfGoodsGroups">
            <column name="idofgoodsgroup"/>
        </many-to-one>
        <many-to-one name="cfProductGroupsByIdofproductgroups"
                     class="ru.axetta.ecafe.processor.core.persistence.CfProductGroups">
            <column name="idofproductgroups"/>
        </many-to-one>
        <many-to-one name="cfProductsByIdofproducts" class="ru.axetta.ecafe.processor.core.persistence.CfProducts">
            <column name="idofproducts"/>
        </many-to-one>
        <set name="cfDishProhibitionExclusionsesByIdofprohibition" inverse="true">
            <key>
                <column name="idofprohibition" not-null="true"/>
            </key>
            <one-to-many not-found="ignore"
                         class="ru.axetta.ecafe.processor.core.persistence.CfDishProhibitionExclusions"/>
        </set>
    </class>
</hibernate-mapping>
        <property insert="false" update="false" name="idofgoodsrequest">
            <column name="idofgoodsrequest" sql-type="int8" length="19"/>
        </property>
        <property insert="false" update="false" name="idofstaff">
            <column name="idofstaff" sql-type="int8" length="19"/>
        </property>
        <property insert="false" update="false" name="idofinternalincomingdocument">
            <column name="idofinternalincomingdocument" sql-type="int8" length="19"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>

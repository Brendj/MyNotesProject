        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="dateofstatechange">
            <column name="dateofstatechange" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="statefrom">
            <column name="statefrom" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="stateto">
            <column name="stateto" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>

        <property name="visitorfullname">
            <column name="visitorfullname" sql-type="varchar" length="110"/>
        </property>
        <property name="doctype">
            <column name="doctype" sql-type="int4" length="10"/>
        </property>
        <property name="docserialnum">
            <column name="docserialnum" sql-type="varchar" length="45"/>

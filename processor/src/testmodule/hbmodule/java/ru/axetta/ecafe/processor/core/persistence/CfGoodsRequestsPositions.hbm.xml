<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfGoodsRequestsPositions"
           table="cf_goods_requests_positions" schema="public">
        <id name="idofgoodsrequestposition">
            <column name="idofgoodsrequestposition" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="idofgoodsrequest">
            <column name="idofgoodsrequest" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="idofgood">
            <column name="idofgood" sql-type="int8" length="19"/>
        </property>
        <property name="idofproducts">
            <column name="idofproducts" sql-type="int8" length="19"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="unitsscale">
            <column name="unitsscale" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="totalcount">
            <column name="totalcount" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="netweight">
            <column name="netweight" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <many-to-one name="cfGoodsRequestsByIdofgoodsrequest"
                     class="ru.axetta.ecafe.processor.core.persistence.CfGoodsRequests">
            <column name="idofgoodsrequest" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
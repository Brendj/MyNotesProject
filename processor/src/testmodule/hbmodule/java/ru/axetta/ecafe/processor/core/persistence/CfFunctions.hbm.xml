<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfFunctions" table="cf_functions" schema="public">
        <id name="idoffunction">
            <column name="idoffunction" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property name="functionname">
            <column name="functionname" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <set name="cfPermissionsesByIdoffunction" inverse="true">
            <key>
                <column name="idoffunction" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfPermissions"/>
        </set>
    </class>
</hibernate-mapping>
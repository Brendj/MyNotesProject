<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfLibvisits" table="cf_libvisits" schema="public">
        <id name="idoflibvisit">
            <column name="idoflibvisit" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property insert="false" update="false" name="idofclient">
            <column name="idofclient" sql-type="int8" length="19"/>
        </property>
        <property name="source">
            <column name="source" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="date">
            <column name="date" sql-type="timestamp" length="29" precision="6" not-null="true"/>
        </property>
        <property name="globalversion">
            <column name="globalversion" sql-type="int8" length="19"/>
        </property>
        <property name="orgowner">
            <column name="orgowner" sql-type="int8" length="19"/>
        </property>
        <property name="deletedstate">
            <column name="deletedstate" sql-type="bool" length="1" not-null="true"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar" length="36" not-null="true"/>
        </property>
        <property name="lastupdate">
            <column name="lastupdate" sql-type="int8" length="19"/>
        </property>
        <property name="deletedate">
            <column name="deletedate" sql-type="int8" length="19"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="sendall">
            <column name="sendall" sql-type="int4" length="10"/>
        </property>
        <many-to-one name="cfClientsByIdofclient" class="ru.axetta.ecafe.processor.core.persistence.CfClients">
            <column name="idofclient"/>
        </many-to-one>
    </class>
</hibernate-mapping>
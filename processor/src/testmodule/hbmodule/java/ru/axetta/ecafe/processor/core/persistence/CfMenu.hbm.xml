<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfMenu" table="cf_menu" schema="public">
        <id name="idofmenu">
            <column name="idofmenu" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property name="idoforg">
            <column name="idoforg" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="menudate">
            <column name="menudate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="menusource">
            <column name="menusource" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="flags">
            <column name="flags" sql-type="int4" length="10" not-null="true"/>
        </property>
        <many-to-one name="cfOrgsByIdoforg" class="ru.axetta.ecafe.processor.core.persistence.CfOrgs">
            <column name="idoforg" not-null="true"/>
        </many-to-one>
        <set name="cfMenudetailsesByIdofmenu" inverse="true">
            <key>
                <column name="idofmenu" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="ru.axetta.ecafe.processor.core.persistence.CfMenudetails"/>
        </set>
    </class>
</hibernate-mapping>
<?xml version='1.0' encoding='utf-8'?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
        xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping http://www.hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd" 
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <class name="ru.axetta.ecafe.processor.core.persistence.CfReportinfo" table="cf_reportinfo" schema="public">
        <id name="idofreportinfo">
            <column name="idofreportinfo" sql-type="bigserial" length="19" not-null="true"/>
        </id>
        <property name="rulename">
            <column name="rulename" sql-type="varchar" length="64" not-null="true"/>
        </property>
        <property name="documentformat">
            <column name="documentformat" sql-type="int4" length="10"/>
        </property>
        <property name="reportname">
            <column name="reportname" sql-type="varchar" length="128" not-null="true"/>
        </property>
        <property name="createddate">
            <column name="createddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="generationtime">
            <column name="generationtime" sql-type="int4" length="10" not-null="true"/>
        </property>
        <property name="startdate">
            <column name="startdate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="enddate">
            <column name="enddate" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="reportfile">
            <column name="reportfile" sql-type="varchar" length="256" not-null="true"/>
        </property>
        <property name="orgnum">
            <column name="orgnum" sql-type="varchar" length="12"/>
        </property>
        <property name="idoforg">
            <column name="idoforg" sql-type="int8" length="19"/>
        </property>
        <property name="tag">
            <column name="tag" sql-type="varchar" length="12"/>
        </property>
    </class>
</hibernate-mapping>
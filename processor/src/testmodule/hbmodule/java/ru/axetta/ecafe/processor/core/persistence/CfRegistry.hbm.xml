<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.CfRegistry" table="cf_registry" schema="public">
        <id name="idofregistry">
            <column name="idofregistry" sql-type="int8" length="19" not-null="true"/>
        </id>
        <property name="version">
            <column name="version" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="clientregistryversion">
            <column name="clientregistryversion" sql-type="int8" length="19" not-null="true"/>
        </property>
        <property name="smsid">
            <column name="smsid" sql-type="bpchar" length="16" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
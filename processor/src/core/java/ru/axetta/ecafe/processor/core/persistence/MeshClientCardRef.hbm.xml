<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.MeshClientCardRef" table="cf_mh_clientcard_ref">
        <id name="idOfRef" column="idOfRef" type="long">
            <generator class="identity" />
        </id>
        <property name="createDate" column="createDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="lastUpdate" column="lastUpdate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="idOfRefInExternalSystem" column="idOfRefInExternalSystem" type="integer"/>
        <property name="deleteState" column="deleteState" type="boolean"/>
        <property name="send" column="isSend" type="boolean"/>
        <many-to-one name="client" class="ru.axetta.ecafe.processor.core.persistence.Client" column="IdOfClient"/>
        <many-to-one name="card" class="ru.axetta.ecafe.processor.core.persistence.Card" column="idofcard"/>
    </class>
</hibernate-mapping>

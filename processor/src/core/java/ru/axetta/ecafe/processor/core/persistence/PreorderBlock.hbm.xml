<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.PreorderBlock" table="cf_preorder_block">
        <id name="idOfPreorderBlock" column="IdOfPreorderBlock" type="long">
            <generator class="identity"/>
        </id>
        <many-to-one name="preorderComplex" class="ru.axetta.ecafe.processor.core.persistence.PreorderComplex"
                     column="IdOfPreorderComplex" not-null="true" fetch="join"/>
        <property name="storno" column="storno" type="ru.axetta.ecafe.processor.core.persistence.BoolType" not-null="true"/>
        <property name="createdDate" column="createddate" type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true"/>
        <property name="lastUpdate" column="lastupdate" type="ru.axetta.ecafe.processor.core.persistence.DateType" />
        <property name="idOfOrgOnCreate" column="idOfOrgOnCreate" type="long" not-null="true"/>
    </class>
</hibernate-mapping>

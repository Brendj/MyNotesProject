<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.MeshTrainingForm" table="cf_kf_training_form">
        <id name="global_id" type="long"/>
        <property name="id" column="id" type="integer" />
        <property name="title" column="title" type="string" />
        <property name="education_form" column="education_form" type="string" />
        <property name="is_deleted" column="is_deleted" type="integer" />
        <property name="archive" column="archive" type="boolean" not-null="true" />

    </class>
</hibernate-mapping>



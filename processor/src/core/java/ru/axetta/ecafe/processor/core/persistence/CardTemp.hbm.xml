<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.CardTemp" table="CF_Cards_Temp">
        <id name="idOfCartTemp" column="IdOfCartTemp" type="long">
            <generator class="identity"/>
        </id>
        <property name="cardNo" column="CardNo" type="long" not-null="true"/>
        <property name="idOfOrg" column="IdOfOrg" type="long" not-null="true"/>
        <property name="cardPrintedNo" column="CardPrintedNo" type="string"/>
    </class>
</hibernate-mapping>



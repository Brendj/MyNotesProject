<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.InteractiveReportData" table="cf_InteractiveReportData">
        <composite-id name="compositeIdOfInteractiveReportData" class="ru.axetta.ecafe.processor.core.persistence.CompositeIdOfInteractiveReportData">
            <key-property name="idOfOrg" type="long" column="IdOfOrg"/>
            <key-property name="idOfRecord" type="long" column="idOfRecord"/>
        </composite-id>

        <property name="value" column="value" type="string" length="255"/>

        <many-to-one name="org" class="ru.axetta.ecafe.processor.core.persistence.Org" column="IdOfOrg" not-null="true"
                     insert="false" update="false"/>
    </class>
</hibernate-mapping>
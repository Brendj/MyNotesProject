<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtUserCatalog" table="cf_wt_usercatalogs">
        <cache usage="transactional"/>
        <id name="idOfUserCatalog">
            <column name="idofusercatalog" sql-type="long"/>
        </id>
        <property name="catalogName">
            <column name="catalogname" sql-type="string" length="128"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="string" length="36"/>
        </property>
        <property name="createDate">
            <column name="createdate" sql-type="long"/>
        </property>
        <property name="lastUpdate">
            <column name="lastupdate" sql-type="long"/>
        </property>
        <property name="version">
            <column name="version" sql-type="long"/>
        </property>
        <property name="deleteState">
            <column name="deletestate" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>
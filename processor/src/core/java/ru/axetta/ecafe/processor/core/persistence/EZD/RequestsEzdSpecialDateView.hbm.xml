<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.EZD.RequestsEzdSpecialDateView" table="cf_ezd_request_special_date">
        <cache usage="transactional"/>
        <id name="id" column="id" type="long">
            <generator class="identity"/>
        </id>
        <property name="specDate" column="date" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="groupname" column="groupname" type="string" length="32"/>
        <property name="idoforg" column="idoforg" type="long" />
        <property name="isweekend" column="isweekend" type="integer" />
    </class>
</hibernate-mapping>


<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtMenu" table="cf_wt_menu">
        <cache usage="transactional"/>
        <id name="idOfMenu">
            <column name="idofmenu" sql-type="long"/>
        </id>
        <property name="menuName">
            <column name="menuname" sql-type="string" length="128"/>
        </property>
        <property name="beginDate">
            <column name="begindate" sql-type="date"/>
        </property>
        <property name="endDate">
            <column name="enddate" sql-type="date"/>
        </property>
        <property name="createDate">
            <column name="createdate" sql-type="date"/>
        </property>
        <property name="lastUpdate">
            <column name="lastupdate" sql-type="date"/>
        </property>
        <property name="version">
            <column name="version" sql-type="long"/>
        </property>
        <property name="deleteState">
            <column name="deletestate" sql-type="integer"/>
        </property>
        <set name="dishes" table="cf_wt_dishes_menu_relationships">
            <key column="idOfMenu"/>
            <many-to-many class="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtDish" column="idOfDish"/>
        </set>
        <set name="orgs" table="cf_wt_menu_org">
            <key column="idOfMenu"/>
            <many-to-many class="ru.axetta.ecafe.processor.core.persistence.Org" column="idOfOrg"/>
        </set>
    </class>
</hibernate-mapping>
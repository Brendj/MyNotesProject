<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.OrgGoodRequest" table="CF_Org_Good_Requests">
        <id name="idOfOrgGoodRequest" column="IdOfOrgGoodRequest" type="long">
            <generator class="identity"/>
        </id>
        <property name="idOfOrg" column="IdOfOrg" type="long" not-null="true"/>
        <property name="day" column="day" type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true" />
        <property name="createdDate" column="createddate" type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true"/>
        <property name="isSent" column="issent" type="ru.axetta.ecafe.processor.core.persistence.BoolType" not-null="true"/>
        <property name="sendDate" column="sendDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
    </class>
</hibernate-mapping>



<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtComplex" table="cf_wt_complexes">
        <id name="idOfComplex" column="idOfComplex" type="long">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="optimizer">pooled-lo</param>
                <param name="increment_size">3</param>
                <param name="sequence_name">cf_wt_complexes_Id_Gen_seq</param>
            </generator>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(256)" length="256"/>
        </property>
        <property name="price">
            <column name="price" sql-type="numeric(10,2)"/>
        </property>
        <property name="beginDate">
            <column name="beginDate" sql-type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true"/>
        </property>
        <property name="endDate">
            <column name="endDate" sql-type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true"/>
        </property>
        <property name="cycleMotion">
            <column name="cycleMotion" sql-type="integer"/>
        </property>
        <property name="dayInCycle">
            <column name="dayInCycle" sql-type="integer"/>
        </property>
        <property name="version">
            <column name="version" sql-type="long"/>
        </property>
        <property name="guid">
            <column name="guid" sql-type="varchar(36)" length="36"/>
        </property>
        <property name="createDate">
            <column name="createDate" sql-type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        </property>
        <property name="lastUpdate">
            <column name="lastUpdate" sql-type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true"/>
        </property>

        <many-to-one name="createdUser" class="ru.axetta.ecafe.processor.core.persistence.User">
            <column name="create_by_id"/>
        </many-to-one>

        <many-to-one name="updatedUser" class="ru.axetta.ecafe.processor.core.persistence.User">
            <column name="update_by_id"/>
        </many-to-one>

        <property name="deleteState">
            <column name="deleteState" sql-type="integer"/>
        </property>

        <many-to-one name="wtComplexGroupItem" lazy="proxy" class="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtComplexGroupItem">
            <column name="idOfComplexGroupItem"/>
        </many-to-one>

        <many-to-one name="wtAgeGroupItem" lazy="proxy" class="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtAgeGroupItem">
            <column name="idOfAgeGroupItem"/>
        </many-to-one>

        <many-to-one name="wtDietType" lazy="proxy" class="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtDietType">
            <column name="idOfDietType"/>
        </many-to-one>

        <many-to-one name="contragent" class="ru.axetta.ecafe.processor.core.persistence.Contragent">
            <column name="idOfContragent"/>
        </many-to-one>

        <many-to-one name="wtOrgGroup" class="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtOrgGroup">
            <column name="idOfOrgGroup"/>
        </many-to-one>

        <property name="composite">
            <column name="composite" sql-type="boolean"/>
        </property>

        <property name="isPortal">
            <column name="is_portal" sql-type="boolean"/>
        </property>

        <property name="startCycleDay">
            <column name="start_cycle_day" sql-type="integer"/>
        </property>

        <property name="barcode">
            <column name="barcode" sql-type="varchar(16)" length="16"/>
        </property>

        <set name="orgs" lazy="false" table="cf_wt_complexes_org">
            <key column="IdOfComplex"/>
            <many-to-many class="ru.axetta.ecafe.processor.core.persistence.Org" column="IdOfOrg"/>
        </set>

        <set name="wtComplexesItems" inverse="true" cascade="all" lazy="false">
            <key>
                <column name="idOfComplexItem"/>
            </key>
            <one-to-many class="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtComplexesItem"/>
        </set>

        <set name="discountRules" lazy="false" table="cf_wt_discountrules_complexes">
            <key column="idOfComplex"/>
            <many-to-many class="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtDiscountRule" column="idOfRule"/>
        </set>

    </class>
</hibernate-mapping>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.EMIAS" table="cf_emias">
        <id name="idOfEMIAS" column="id" type="long">
            <generator class="identity"/>
        </id>
        <property name="guid" column="guid" type="string" length="128"/>
        <property name="idEventEMIAS" column="idEventEMIAS" type="long"/>
        <property name="typeEventEMIAS" column="typeEventEMIAS" type="long"/>
        <property name="dateLiberate" column="dateLiberate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="startDateLiberate" column="startDateLiberate"
                  type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="endDateLiberate" column="endDateLiberate"
                  type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="createDate" column="createDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="updateDate" column="updateDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="accepted" column="accepted" type="boolean"/>
        <property name="deletedemiasid" column="deletedemiasid" type="long"/>
    </class>
</hibernate-mapping>



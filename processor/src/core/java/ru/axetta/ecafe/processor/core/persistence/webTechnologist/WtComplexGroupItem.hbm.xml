<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.axetta.ecafe.processor.core.persistence.webTechnologist.WtComplexGroupItem" table="cf_wt_complex_group_items">
        <cache usage="transactional"/>
        <id name="idOfComplexGroupItem">
            <column name="idofcomplexgroupitem" sql-type="long"/>
        </id>
        <property name="description">
            <column name="description" sql-type="string" length="255"/>
        </property>
    </class>
</hibernate-mapping>
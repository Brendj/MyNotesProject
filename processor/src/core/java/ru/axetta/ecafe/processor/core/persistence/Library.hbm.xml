<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.Library" table="cf_library">
        <cache usage="transactional"/>
        <id name="id" column="idrequestsezd" type="long">
            <generator class="identity"/>
        </id>
        <property name="guid" column="guid" type="string"/>
        <property name="libraryCode" column="libraryCode" type="long"/>
        <property name="libraryName" column="libraryName" type="string"/>
        <property name="libraryAdress" column="libraryAdress" type="string"/>
        <property name="accessTime" column="accessTime" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="createDate" column="createddate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
    </class>
</hibernate-mapping>



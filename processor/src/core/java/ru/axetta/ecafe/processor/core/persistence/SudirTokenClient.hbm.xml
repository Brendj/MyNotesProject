<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.SudirTokenClient" table="CF_Sudir_Token_Client">
        <id name="idOfSudirTokenClient" column="IdOfSudirTokenClient" type="long">
            <generator class="identity"/>
        </id>
        <property name="access_token" column="access_token" type="string" length="64" not-null="true"/>
        <property name="contractId" column="contractId" type="long" not-null="true"/>
    </class>
</hibernate-mapping>



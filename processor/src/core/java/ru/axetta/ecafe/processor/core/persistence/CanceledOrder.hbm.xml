<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.CanceledOrder" table="CF_CanceledOrders">
        <id name="idOfCanceledOrder" column="idOfCanceledOrder" type="long">
            <generator class="identity"/>
        </id>
        <property name="idOfOrder" column="IdOfOrder" type="long" not-null="true" update="false" insert="false"/>
        <many-to-one name="org" class="ru.axetta.ecafe.processor.core.persistence.Org" column="IdOfOrg" not-null="true" insert="false" update="false"/>
        <many-to-one name="order" class="ru.axetta.ecafe.processor.core.persistence.Order" not-null="true" update="false">
            <column name="IdOfOrg"/>
            <column name="IdOfOrder"/>
        </many-to-one>
        <property name="idOfTransaction" column="idOfTransaction" type="long" update="false"/>
        <property name="createTime" column="CreatedDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"
                  not-null="true" update="false"/>
    </class>
</hibernate-mapping>
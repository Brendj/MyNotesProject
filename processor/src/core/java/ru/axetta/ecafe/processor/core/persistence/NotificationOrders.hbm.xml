<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.NotificationOrders" table="cf_notification_orders">
        <id name="idOfNotificationOrders" column="idOfNotificationOrders" type="long">
            <generator class="identity"/>
        </id>
        <property name="idOfOrder" column="idOfOrder" type="long"/>
        <property name="idOfClient" column="idOfClient" type="long"/>
        <property name="createddate" column="createddate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="sended" column="sended" type="ru.axetta.ecafe.processor.core.persistence.BoolType"/>
    </class>
</hibernate-mapping>



<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.CheckSums" table="CF_CheckSums" optimistic-lock="version">
        <id name="idOfCheckSums" column="idOfCheckSums" type="long">
            <generator class="identity"/>
        </id>
        <property name="checkSumsDate" column="CheckSumsDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"
                  not-null="true" update="false"/>
        <property name="distributionVersion" column="DistributionVersion" type="string" length="20"/>
        <property name="checkSumsMd5" column="CheckSumsMd5" type="string" length="32"/>
    </class>
</hibernate-mapping>
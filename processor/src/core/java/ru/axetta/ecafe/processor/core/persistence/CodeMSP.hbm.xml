<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.CodeMSP" table="cf_code_msp">
        <id name="idOfCode" column="idOfCode" type="long">
            <generator class="identity"/>
        </id>
        <property name="code" column="code" type="integer" not-null="true"/>
        <property name="description" column="description" type="string" length="512" not-null="true"/>
        <many-to-one name="categoryDiscount" class="ru.axetta.ecafe.processor.core.persistence.CategoryDiscount"
                     column="idofcategorydiscount" lazy="false" not-null="true"/>
    </class>
</hibernate-mapping>
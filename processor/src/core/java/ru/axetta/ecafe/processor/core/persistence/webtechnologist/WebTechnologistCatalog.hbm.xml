<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.webtechnologist.WebTechnologistCatalog" table="cf_webTechnologist_catalogs">
        <cache usage="transactional"/>
        <id name="idOfWebTechnologistCatalog" column="idOfWebTechnologistCatalog">
            <generator class="identity"/>
        </id>
        <property name="catalogName" column="catalogName" type="string" not-null="true"/>
        <property name="GUID" column="GUID" type="string" not-null="true"/>
        <property name="createDate" column="createDate" type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true"/>
        <property name="lastUpdate" column="lastUpdate" type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true"/>
        <property name="version" column="version" type="long" not-null="true"/>
        <property name="deleteState" column="deleteState " type="ru.axetta.ecafe.processor.core.persistence.BoolType" not-null="true"/>
        <many-to-one name="userCreator" column="idOfUserCreator" class="ru.axetta.ecafe.processor.core.persistence.User" not-null="true" lazy="false"/>
        <set table="cf_webTechnologist_catalog_items" name="items" lazy="false">
            <key column="idOfWebTechnologistCatalog"/>
            <one-to-many class="ru.axetta.ecafe.processor.core.persistence.webtechnologist.WebTechnologistCatalogItem"/>
        </set>
    </class>
</hibernate-mapping>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.GroupNamesToOrgs" table="Cf_GroupNames_To_Orgs">
        <id name="idOfGroupNameToOrg" column="IdOfGroupNameToOrg" type="long">
            <generator class="identity"/>
        </id>
        <many-to-one name="idOfOrg" class="ru.axetta.ecafe.processor.core.persistence.Org" column="IdOfOrg"
                     not-null="true"/>
        <property name="idOfMainOrg" column="IdOfMainOrg" type="long"/>
        <property name="mainBuilding" column="MainBuilding" type="int"/>
        <property name="groupName" column="GroupName" type="string"/>
    </class>
</hibernate-mapping>
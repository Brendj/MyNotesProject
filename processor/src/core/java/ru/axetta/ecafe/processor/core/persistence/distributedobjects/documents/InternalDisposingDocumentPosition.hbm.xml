<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.distributedobjects.documents.InternalDisposingDocumentPosition" table="cf_internal_disposing_document_positions">
        <id name="globalId" column="IdOfInternalDisposingDocumentPositions" type="long">
            <generator class="identity"/>
        </id>
        <!-- Begin Global params -->
        <property name="globalVersion" column="GlobalVersion" type="long"/>

        <property name="deletedState" column="DeletedState" type="boolean"/>

        <property name="guid" column="GUID" type="string" length="36" not-null="true" unique="true"/>

        <property name="lastUpdate" column="LastUpdate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="deleteDate" column="DeleteDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="createdDate" column="CreatedDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"
        not-null="true" update="false"/>
        <property name="sendAll" column="SendAll" type="ru.axetta.ecafe.processor.core.persistence.BoolType"/>
        <!-- End Global params -->

        <property name="orgOwner" column="OrgOwner" type="long"/>
        <property name="unitsScale" column="UnitsScale" type="integer" not-null="true"/>
        <property name="totalCount" column="TotalCount" type="long" not-null="true"/>
        <property name="netWeight" column="NetWeight" type="long" not-null="true"/>
        <property name="disposePrice" column="DisposePrice" type="long" not-null="true"/>
        <property name="nds" column="NDS" type="long" not-null="true"/>

        <many-to-one name="internalDisposingDocument" class="ru.axetta.ecafe.processor.core.persistence.distributedobjects.documents.InternalDisposingDocument" fetch="select" lazy="false">
            <column name="IdOfInternalDisposingDocument" not-null="true" />
        </many-to-one>

        <many-to-one name="tradeMaterialGood" class="ru.axetta.ecafe.processor.core.persistence.distributedobjects.products.TradeMaterialGood" fetch="select" lazy="false">
            <column name="IdOfTradeMaterialGood" not-null="true" />
        </many-to-one>

        <many-to-one name="good" class="ru.axetta.ecafe.processor.core.persistence.distributedobjects.products.Good" fetch="select" lazy="false">
            <column name="IdOfGood" not-null="true" />
        </many-to-one>

    </class>
</hibernate-mapping>
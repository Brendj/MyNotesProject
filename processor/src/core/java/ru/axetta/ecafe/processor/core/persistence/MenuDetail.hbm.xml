<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.MenuDetail" table="CF_MenuDetails">
        <id name="idOfMenuDetail" column="IdOfMenuDetail" type="long">
            <generator class="hilo">
                <param name="table">CF_Generators</param>
                <param name="column">IdOfMenuDetail</param>
                <param name="max_lo">3</param>
            </generator>
        </id>
        <many-to-one name="menu" class="ru.axetta.ecafe.processor.core.persistence.Menu" column="IdOfMenu"
                     not-null="true"
                     update="false"/>
        <property name="menuPath" column="MenuPath" type="string" length="256" not-null="true"
                  update="false"/>
        <property name="menuDetailName" column="MenuDetailName" type="string" length="256" not-null="true"
                  update="false"/>
        <property name="groupName" column="GroupName" type="string" length="60" not-null="true" update="false"/>
        <property name="menuDetailOutput" column="MenuDetailOutput" type="string" length="32" not-null="true"
                  update="false"/>
        <property name="menuOrigin" column="MenuOrigin" type="int" not-null="true"/>
        <property name="localIdOfMenu" column="LocalIdOfMenu" type="long"/>
        <property name="availableNow" column="AvailableNow" type="int" not-null="true"/>
        <property name="price" column="Price" type="long" not-null="true" update="false"/>
        <property name="fat" column="Fat" type="double" update="false"/>
        <property name="carbohydrates" column="Carbohydrates" type="double" update="false"/>
        <property name="calories" column="Calories" type="double" update="false"/>
        <property name="vitB1" column="VitB1" type="double" update="false"/>
        <property name="vitC" column="VitC" type="double" update="false"/>
        <property name="vitA" column="VitA" type="double" update="false"/>
        <property name="vitE" column="VitE" type="double" update="false"/>
        <property name="minCa" column="MinCa" type="double" update="false"/>
        <property name="minP" column="MinP" type="double" update="false"/>
        <property name="minMg" column="MinMg" type="double" update="false"/>
        <property name="minFe" column="MinFe" type="double" update="false"/>
    </class>
</hibernate-mapping>



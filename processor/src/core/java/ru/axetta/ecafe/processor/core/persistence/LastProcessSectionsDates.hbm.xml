<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.LastProcessSectionsDates" table="cf_lastprocesssectionsdates">
        <composite-id name="compositeIdOfLastProcessSectionsDates"
                      class="ru.axetta.ecafe.processor.core.persistence.CompositeIdOfLastProcessSectionsDates">
            <key-property name="idOfOrg" type="long" column="IdOfOrg"/>
            <key-property name="type" type="integer" column="Type"/>
        </composite-id>
        <many-to-one name="org" class="ru.axetta.ecafe.processor.core.persistence.Org" column="IdOfOrg"
                     not-null="true" insert="false" update="false"/>
        <property name="date" column="Date" type="ru.axetta.ecafe.processor.core.persistence.DateType"
                  not-null="true" update="false"/>
    </class>
</hibernate-mapping>



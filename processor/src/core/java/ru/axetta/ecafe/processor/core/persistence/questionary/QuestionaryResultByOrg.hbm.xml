<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.questionary.QuestionaryResultByOrg" table="CF_QA_QuestionaryResultByOrg">
        <id name="idOfQuestionaryResultByOrg" column="IdOfQuestionaryResultByOrg" type="long">
            <generator class="identity"/>
        </id>

        <many-to-one name="org" class="ru.axetta.ecafe.processor.core.persistence.Org" column="IdOfOrg"
                     not-null="true" update="false" lazy="proxy"/>
        <many-to-one name="answer" class="ru.axetta.ecafe.processor.core.persistence.questionary.Answer" column="IdOfAnswer"
                     not-null="true" update="false" lazy="proxy"/>
        <many-to-one name="questionary" class="ru.axetta.ecafe.processor.core.persistence.questionary.Questionary" column="IdOfQuestionary"
                     not-null="true" update="false" lazy="proxy"/>
        <property name="count" column="Count" type="integer"/>
        <property name="updatedDate" column="UpdatedDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>

    </class>
</hibernate-mapping>
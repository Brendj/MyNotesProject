<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.SfcUserOrgs" table="CF_SfcUserOrgs" optimistic-lock="version">
        <id name="idOfSfcUserOrg" column="idofsfcuserorg" type="long">
            <generator class="identity"/>
        </id>
        <many-to-one name="user" class="ru.axetta.ecafe.processor.core.persistence.User" column="idofuser"
                     not-null="false" fetch="select" lazy="proxy"/>
        <many-to-one name="org" class="ru.axetta.ecafe.processor.core.persistence.Org" column="idoforg"
                     not-null="false"/>
    </class>
</hibernate-mapping>
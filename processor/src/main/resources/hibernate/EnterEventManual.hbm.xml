<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.EnterEventManual" table="CF_EnterEvents_Manual">
        <id name="idOfEnterEventManual" column="IdOfEnterEventManual" type="long">
            <generator class="identity"/>
        </id>
        <property name="enterName" column="EnterName" type="string" not-null="true"/>
        <property name="evtDateTime" column="EvtDateTime" type="ru.axetta.ecafe.processor.core.persistence.DateType"
                  not-null="true"/>
        <property name="idOfOrg" column="IdOfOrg" type="long" not-null="true"/>
        <property name="idOfClient" column="IdOfClient" type="long" not-null="true"/>
    </class>
</hibernate-mapping>



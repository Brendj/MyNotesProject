<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.CardSign" table="cf_card_signs">
        <id name="idOfCardSign" column="IdOfCardSign" type="integer">
            <generator class="identity"/>
        </id>
        <property name="signType" column="signType" type="integer" not-null="true"/>
        <property name="signData" column="signData" type="byte[]" not-null="true"/>
        <property name="manufacturerCode" column="manufacturerCode" type="integer" not-null="true" unique="true"/>
        <property name="manufacturerName" column="manufacturerName" type="string" length="200" not-null="true"/>
        <property name="privatekeycard" column="privatekeycard" type="byte[]" not-null="true"/>
        <property name="publickeyprovider" column="publickeyprovider" type="byte[]" not-null="true"/>
        <property name="signtypeprov" column="signtypeprov" type="integer" not-null="true"/>
        <property name="newtypeprovider" column="newtypeprovider" type="boolean" not-null="true"/>
        <property name="deleted" column="deleted" type="boolean" not-null="true"/>

    </class>
</hibernate-mapping>



<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.ApplicationForFoodDiscount" table="cf_applications_for_food_discount">
        <id name="idOfAppDiscount" column="idofappdiscount" type="long">
            <generator class="identity"/>
        </id>
        <many-to-one name="applicationForFood" class="ru.axetta.ecafe.processor.core.persistence.ApplicationForFood" column="idofapplicationforfood"
                     not-null="true" fetch="join"/>
        <property name="dtisznCode" column="DTISZNCode" type="integer"/>
        <property name="confirmed" column="confirmed" type="ru.axetta.ecafe.processor.core.persistence.BoolType" not-null="true"/>
        <property name="startDate" column="startdate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="endDate" column="enddate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="appointedMSP" column="appointedMSP" type="ru.axetta.ecafe.processor.core.persistence.BoolType" not-null="true"/>
    </class>
</hibernate-mapping>

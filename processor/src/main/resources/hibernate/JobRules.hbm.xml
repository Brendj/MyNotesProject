<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.JobRules" table="CF_JobRules" optimistic-lock="version">
        <id name="idOfJobRule" column="idofjobrule" type="long">
            <generator class="identity"/>
        </id>
        <many-to-one name="reportHandleRule" class="ru.axetta.ecafe.processor.core.persistence.ReportHandleRule" column="idofreporthandlerule"
                     not-null="false" fetch="select" lazy="proxy"/>
        <many-to-one name="schedulerJob" class="ru.axetta.ecafe.processor.core.persistence.SchedulerJob" column="idofschedulerjob"
                     not-null="false"/>
    </class>
</hibernate-mapping>
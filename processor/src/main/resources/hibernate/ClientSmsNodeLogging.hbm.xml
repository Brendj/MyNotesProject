<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.ClientSmsNodeLogging" table="cf_clientsms_node_logging">
        <id name="id" column="id" type="long">
            <generator class="identity"/>
        </id>
        <property name="idOfSms" column="idOfSms" type="string"/>
        <property name="params" column="params" type="string"/>
        <property name="nodeName" column="nodename" type="string"/>
        <property name="createDate" column="createDate"
                  type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
    </class>
</hibernate-mapping>



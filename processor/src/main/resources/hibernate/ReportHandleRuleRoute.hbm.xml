<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.ReportHandleRuleRoute" table="CF_ReportHandleRules_routes">
        <id name="idOfReportHandleRuleRoute" column="idofreporthandleruleroute" type="long">
            <generator class="identity"/>
        </id>
        <property name="route" column="Route" type="string" length="128" not-null="true"/>
        <many-to-one name="reportHandleRule" class="ru.axetta.ecafe.processor.core.persistence.ReportHandleRule"
                     column="idofreporthandlerule" not-null="true"/>
    </class>
</hibernate-mapping>



<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.ClientGuardianNotificationSetting" table="cf_client_guardian_notificationsettings">
        <id name="IdOfSetting" column="idOfSetting"  type="long">
            <generator class="identity"/>
        </id>
        <many-to-one name="clientGuardian" class="ru.axetta.ecafe.processor.core.persistence.ClientGuardian" column="IdOfClientGuardian"
                         not-null="true" update="false"/>
        <property name="NotifyType" column="notifyType" type="long"/>
        <property name="CreatedDate" column="CreatedDate" type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true"/>
    </class>
</hibernate-mapping>
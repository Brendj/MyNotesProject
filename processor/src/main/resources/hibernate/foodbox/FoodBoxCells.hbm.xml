<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.foodbox.FoodBoxCells" table="cf_foodbox_cells">
        <id name="foodboxesid" column="foodboxesid" type="long">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="optimizer">pooled-lo</param>
                <param name="increment_size">1</param>
                <param name="sequence_name">cf_foodbox_cells_foodboxesid_seq</param>
            </generator>
        </id>
        <property name="fbId" column="fbId" type="integer"/>
        <property name="totalcellscount" column="totalcellscount" type="integer"/>
        <property name="busycells" column="busycells" type="integer"/>
        <many-to-one name="org" class="ru.axetta.ecafe.processor.core.persistence.Org" column="idoforg"
                     not-null="true" update="false"/>
        <property name="createDate" column="createdate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="updateDate" column="updatedate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
    </class>
</hibernate-mapping>
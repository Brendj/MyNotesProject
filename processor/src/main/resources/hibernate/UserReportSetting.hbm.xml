<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.UserReportSetting" table="cf_user_report_settings" optimistic-lock="version">
        <id name="idOfUserReportSetting" column="idOfUserReportSetting" type="long">
            <generator class="identity"/>
        </id>
        <property name="numberOfReport" column="numberOfReport" type="integer" not-null="true"/>
        <property name="settings" column="settings" type="string" not-null="true"/>
        <many-to-one name="user" column="idOfUser" class="ru.axetta.ecafe.processor.core.persistence.User" update="false"/>
    </class>
</hibernate-mapping>



<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.distributedobjects.DOConflict" table="cf_do_conflicts" optimistic-lock="version">
        <id name="IdOfDoConflict" column="IdOfDoConflict" type="long">
            <generator class="identity"/>
        </id>

        <property name="idOfOrg" column="idOfOrg" type="long"/>
        <property name="distributedObjectClassName" column="DistributedObjectClassName" type="string" length="64"/>
        <property name="createConflictDate" column="CreateConflictDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="gVersionInc" column="gVersion_inc" type="long"/>
        <property name="gVersionCur" column="gVersion_cur" type="long"/>
        <property name="gVersionResult" column="gVersion_result" type="long"/>
        <property name="valueInc" column="val_inc" type="string" length="16548"/>
        <property name="valueCur" column="val_cur" type="string" length="16548"/>
    </class>
</hibernate-mapping>
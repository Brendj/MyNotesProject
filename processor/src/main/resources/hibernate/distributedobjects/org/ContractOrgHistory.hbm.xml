<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.distributedobjects.org.ContractOrgHistory" table="cf_contract_org">
        <id name="idOfContractOrg" column="IdOfContractOrg" type="long">
            <generator class="identity"/>
        </id>
        <property name="lastVersionOfContract" column="LastVersionOfContract" type="long" not-null="true"/>
        <property name="createdDate" column="CreatedDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"
                  not-null="true" update="false"/>
        <many-to-one name="contract" lazy="proxy" class="ru.axetta.ecafe.processor.core.persistence.distributedobjects.org.Contract" cascade="delete">
            <column name="idOfContract"/>
        </many-to-one>
        <many-to-one name="org" lazy="proxy" class="ru.axetta.ecafe.processor.core.persistence.Org" cascade="delete">
            <column name="idOfOrg"/>
        </many-to-one>
    </class>
</hibernate-mapping>



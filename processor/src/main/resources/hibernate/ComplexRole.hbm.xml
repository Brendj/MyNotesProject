<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.ComplexRole" table="CF_ComplexRoles">
        <id name="idOfRole" column="IdOfRole" type="long"/>
        <property name="roleName" column="RoleName" type="string" not-null="true" update="true"/>
        <property name="extendRoleName" column="ExtendRoleName" type="string" update="true"/>
    </class>
</hibernate-mapping>

        <property name="c0" type="long" column="C0"/>
        <property name="c1" type="long" column="C1"/>
        <property name="c2" type="long" column="C2"/>
        <property name="c3" type="long" column="C3"/>
        <property name="c4" type="long" column="C4"/>
        <property name="c5" type="long" column="C5"/>
        <property name="c6" type="long" column="C6"/>
        <property name="c7" type="long" column="C7"/>
        <property name="c8" type="long" column="C8"/>
        <property name="c9" type="long" column="C9"/>

        <many-to-one name="diaryClass0" class="ru.axetta.ecafe.processor.core.persistence.DiaryClass" insert="false"
                     update="false">

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.RegistryChangeEmployee" table="CF_RegistryChange_Employee" optimistic-lock="version">
        <id name="idOfRegistryChange" column="IdOfRegistryChange" type="long">
            <generator class="identity"/>
        </id>

        <property name="idOfOrg" column="IdOfOrg" type="long" not-null="true"/>
        <property name="createDate" column="CreateDate" type="long" not-null="true"/>
        <property name="clientGUID" column="ClientGUID" type="string" length="40" not-null="false"/>

        <property name="firstName" column="FirstName" type="string" length="64" not-null="true"/>
        <property name="secondName" column="SecondName" type="string" length="128" not-null="true"/>
        <property name="surname" column="Surname" type="string" length="128" not-null="true"/>
        <property name="groupName" column="GroupName" type="string" length="3" not-null="true"/>

        <property name="idOfMigrateOrgTo" column="IdOfMigrateOrgTo" type="long" not-null="false"/>
        <property name="idOfMigrateOrgFrom" column="IdOfMigrateOrgFrom" type="long" not-null="false"/>
        <property name="firstNameFrom" column="FirstNameFrom" type="string" length="64"/>

        <property name="secondNameFrom" column="SecondNameFrom" type="string" length="128"/>
        <property name="surnameFrom" column="SurnameFrom" type="string" length="128"/>
        <property name="groupNameFrom" column="GroupNameFrom" type="string" length="3"/>

        <property name="idOfClient" column="IdOfClient" type="long" not-null="false"/>
        <property name="operation" column="Operation" type="int" not-null="true"/>
        <property name="type" column="Type" type="int" not-null="true"/>
        <property name="applied" column="Applied" type="boolean" not-null="true"/>
        <property name="error" column="Error" type="string" length="256"/>
        <property name="notificationId" column="NotificationId" type="string" length="15"/>
        <property name="gender" column="Gender" type="int"/>
        <property name="birthDate" column="BirthDate" type="long"/>
        <property name="genderFrom" column="GenderFrom" type="int"/>
        <property name="birthDateFrom" column="BirthDateFrom" type="long"/>
        <property name="checkBenefits" column="CheckBenefits" type="ru.axetta.ecafe.processor.core.persistence.BoolType"/>
        <property name="benefitDSZN" column="BenefitDSZN" type="string" length="128"/>
        <property name="newDiscounts" column="NewDiscounts" type="string" length="128"/>
        <property name="benefitDSZNFrom" column="BenefitDSZNFrom" type="string" length="128"/>
        <property name="oldDiscounts" column="OldDiscounts" type="string" length="128"/>
        <property name="guardiansCount" column="GuardiansCount" type="int"/>
        <property name="ageTypeGroup" column="AgeTypeGroup" type="string" length="128"/>
        <property name="ageTypeGroupFrom" column="AgeTypeGroupFrom" type="string" length="128"/>
    </class>
</hibernate-mapping>



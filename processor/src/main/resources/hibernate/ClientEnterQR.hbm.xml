<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.ClientEnterQR" table="cf_client_qr_code">
        <id name="idQRCode" column="idQRCode" type="long">
            <generator class="identity"/>
        </id>
        <many-to-one name="client" class="ru.axetta.ecafe.processor.core.persistence.Client" column="IdOfClient"
                     not-null="true" update="false"/>
        <property name="qr" column="qr" type="byte[]" not-null="true"/>
        <property name="startDate" column="startDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="endDate" column="endDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
        <property name="createDate" column="createDate" type="ru.axetta.ecafe.processor.core.persistence.DateType"/>
    </class>
</hibernate-mapping>



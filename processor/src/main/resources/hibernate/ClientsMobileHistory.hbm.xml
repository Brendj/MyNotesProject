<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.ClientsMobileHistory" table="cf_clients_mobile_history">
        <id name="ifofclientsmobilehistory" column="ifofclientsmobilehistory" type="long">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="optimizer">pooled-lo</param>
                <param name="increment_size">1</param>
                <param name="sequence_name">cf_clients_mobile_history_id</param>
            </generator>
        </id>
        <many-to-one name="client" class="ru.axetta.ecafe.processor.core.persistence.Client" column="idofclient"
                     update="false"/>
        <property name="oldmobile" column="oldmobile" type="string"/>
        <property name="newmobile" column="newmobile" type="string"/>
        <property name="action" column="action" type="string"/>
        <property name="createdate" column="createdate" type="ru.axetta.ecafe.processor.core.persistence.DateType"
                  not-null="true"/>
        <property name="source" column="source" type="string"/>
        <property name="showing" column="showing" type="string"/>
        <many-to-one name="user" class="ru.axetta.ecafe.processor.core.persistence.User" column="idofusers"
                     update="false"/>
        <property name="staffguid" column="staffguid" type="string"/>
        <many-to-one name="org" class="ru.axetta.ecafe.processor.core.persistence.Org" column="idoforg"
                     update="false"/>
        <property name="oldssoid" column="oldssoid" type="string"/>
    </class>
</hibernate-mapping>



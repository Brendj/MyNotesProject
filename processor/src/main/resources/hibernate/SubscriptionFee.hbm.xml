<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.SubscriptionFee" table="CF_SubscriptionFee">
        <id name="idOfSubscriptionFee" column="IdOfSubscriptionFee" type="long">
            <generator class="identity"/>
        </id>
        <property name="subscriptionYear" type="integer" column="SubscriptionYear" not-null="true"/>
        <property name="periodNo" type="integer" column="PeriodNo" not-null="true"/>
        <many-to-one name="transaction" class="ru.axetta.ecafe.processor.core.persistence.AccountTransaction"
                     column="IdOfTransaction" not-null="true" update="false" fetch="select"/>
        <property name="createTime" column="CreateTime" type="ru.axetta.ecafe.processor.core.persistence.DateType"
                  not-null="true" update="false"/>
        <property name="subscriptionSum" column="SubscriptionSum" type="long" not-null="true" update="false"/>
        <property name="subscriptionType" column="SubscriptionType" type="integer" not-null="true"/>
    </class>
</hibernate-mapping>



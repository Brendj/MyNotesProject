<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.EZD.RequestsEzdView" table="cf_ezd_request">
        <cache usage="transactional"/>
        <id name="id" column="id" type="long">
            <generator class="identity"/>
        </id>
        <property name="idoforg" column="idoforg" type="long" />
        <property name="orgguid" column="guid" type="string" length="32"/>
        <property name="ekisid" column="ekisid" type="long" />
        <property name="usewebarm" column="useWebArm" type="boolean"/>
        <property name="groupname" column="groupname" type="string" length="32"/>
    </class>
</hibernate-mapping>


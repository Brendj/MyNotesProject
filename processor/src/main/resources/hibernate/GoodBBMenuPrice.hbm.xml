<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.GoodBBMenuPrice" table="cf_good_bb_menu_price">
        <id name="idOfGoodBBMenuPrice" column="idOfGoodBBMenuPrice" type="long">
            <generator class="identity"/>
        </id>
        <property name="idOfBasicGood" column="idOfBasicGood" type="long" not-null="true"/>
        <property name="idOfConfigurationProvider" column="idOfConfigurationProvider" type="long" not-null="true"/>
        <property name="menuDate" column="menuDate" type="ru.axetta.ecafe.processor.core.persistence.DateType" not-null="true"/>
        <property name="price" column="price" type="long" />
        <property name="menuDetailName" column="menuDetailName" type="string" not-null="true" length="512"/>
    </class>
</hibernate-mapping>

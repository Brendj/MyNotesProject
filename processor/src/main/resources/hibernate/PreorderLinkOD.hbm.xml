<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.axetta.ecafe.processor.core.persistence.PreorderLinkOD" table="cf_preorder_linkod">
        <id name="idOfPreorderLinkOD" column="IdOfPreorderLinkOD" type="long">
            <generator class="identity"/>
        </id>
        <property name="preorderGuid" column="preorderGuid" type="string" length="36"/>
        <property name="idOfOrg" column="idOfOrg" type="long" />
        <property name="idOfOrder" column="idOfOrder" type="long" />
        <property name="idOfOrderDetail" column="idOfOrderDetail" type="long" />
        <property name="qty" column="qty" type="long" not-null="true "/>
        <property name="price" column="price" type="long" not-null="true" />
        <property name="itemCode" column="itemcode" type="string" length="32" />
    </class>
</hibernate-mapping>
